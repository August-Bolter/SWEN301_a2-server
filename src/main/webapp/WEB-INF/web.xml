<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <servlet>
    <servlet-name>LogsServlet</servlet-name>
    <servlet-class>nz.ac.vuw.swen301.a2.server.LogsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StatsCSVServlet</servlet-name>
    <servlet-class>nz.ac.vuw.swen301.a2.server.StatsCSVServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StatsServlet</servlet-name>
    <servlet-class>nz.ac.vuw.swen301.a2.server.StatsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StatsPNGServlet</servlet-name>
    <servlet-class>nz.ac.vuw.swen301.a2.server.StatsPNGServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogsServlet</servlet-name>
    <url-pattern>/logs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StatsCSVServlet</servlet-name>
    <url-pattern>/statscsv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StatsServlet</servlet-name>
    <url-pattern>/stats</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StatsPNGServlet</servlet-name>
    <url-pattern>/statspng</url-pattern>
  </servlet-mapping>
</web-app>
